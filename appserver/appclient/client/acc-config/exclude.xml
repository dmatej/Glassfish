<FindBugsFilter>
    <!--
    Ignore this error in code generated by JAXB, which can
    generate s.toString() calls for a string "s".
    -->
    <Match>
        <Package name="org.glassfish.appclient.client.acc.config"/>
    <Bug pattern="DM_STRING_TOSTRING"/>
    </Match>

    <!--
    Ignore this error in code generated by JAXB.  The DTD
    sun-application-client-container_1_2.dtd file uses both
    <classname> and <class-name>, and we can't fix that for compatibility.
    -->
    <Match>
        <Class name="org.glassfish.appclient.client.acc.config.AuthRealm"/>
    <Bug pattern="NM_CONFUSING"/>
    </Match>
</FindBugsFilter>
