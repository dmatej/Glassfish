       </env-entry>
   <env-entry>
         <env-entry-name>envEntry3</env-entry-name>
         <env-entry-type>java.lang.Integer</env-entry-type>
         <lookup-name>java:app/env/value1</lookup-name>
       </env-entry>

</application-client>

